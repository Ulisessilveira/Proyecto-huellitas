{"ast":null,"code":"import HeaderComponent from '@/components/layouts/HeaderComponent.vue';\nimport SidebarComponent from '@/components/layouts/SidebarComponent.vue';\nimport UsersAddComponent from './UsersAddComponent.vue';\nimport UsersEditComponent from './UsersEditComponent.vue';\nimport axios from 'axios';\nexport default {\n  name: 'UsersComponent',\n  components: {\n    HeaderComponent,\n    SidebarComponent,\n    UsersAddComponent,\n    UsersEditComponent\n  },\n  data() {\n    return {\n      add: false,\n      edit: false,\n      data: []\n    };\n  },\n  created() {\n    axios.get(\"http://localhost:8000/api/users\").then(result => {\n      if (result.data.status == 'success') {\n        this.data = result.data.data;\n      }\n    });\n  }\n};","map":{"version":3,"names":["HeaderComponent","SidebarComponent","UsersAddComponent","UsersEditComponent","axios","name","components","data","add","edit","created","get","then","result","status"],"sources":["C:\\Users\\saulb\\Documents\\GitHub\\Proyecto-huellitas\\src\\views\\admin\\users\\UsersComponent.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <HeaderComponent/>\r\n        <div class=\"row\">\r\n            <aside class=\"col-2\">\r\n                <SidebarComponent/>\r\n                <!--END SIDEBAR-->\r\n            </aside>\r\n            <main class=\"col-10\">\r\n                <h1 class=\"d-flex justify-content-between\">\r\n                    <div class=\"h4 py-2\">Users</div>\r\n                    <div class=\"px-4\">\r\n                        <buttton class=\"btn btn-sm\"\r\n                        :class=\"{'btn-outline-success':!add,'btn-outline-danger':add}\"\r\n                        v-on:click=\" add=!add\" v-if=\"!edit\">\r\n                            <span v-if=\"!add\"><i class=\"fa fa-plus\"></i> Add</span>\r\n                            <span v-if=\"add\"><i class=\"fa fa-times\"></i> Cancel</span>\r\n                        </buttton>\r\n\r\n                        <button v-else class=\"btn btn-sm btn-outline-danger\"\r\n                        v-on:click=\"edit=!edit\">\r\n                            <span v-if=\"edit\"><i class=\"fa fa-times\"></i> Cancel edit</span>\r\n                        </button>\r\n                    </div>\r\n                </h1>\r\n                <div class=\"row\" v-if=\"!add && !edit\">\r\n                    <div class=\"container-flex px-5\">\r\n                        <div class=\"row\">\r\n                            <table class=\"table\">\r\n                                <thead>\r\n                                    <tr>\r\n                                        <th scope=\"col\" class=\"text-center\"></th>\r\n                                        <th scope=\"col\">Name</th>\r\n                                        <th scope=\"col\">Email</th>\r\n                                        <th scope=\"col\">Phone</th>\r\n                                        <th scope=\"col\">Address</th>\r\n                                        <th scope=\"col\" class=\"text-center\">Options</th>\r\n                                    </tr>\r\n                                </thead>\r\n                                <tbody>\r\n                                    <tr v-for=\"x in data\" v-bind:key=\"x.id\">\r\n                                        <td class=\"text-center\"><img class=\"img-Profile rounded-circle\" :src=\"'http://localhost:8000/img/users/'+x.img\" alt=\"\"></td>\r\n                                        <td>{{x.name}}</td>\r\n                                        <td>{{x.email}}</td>\r\n                                        <td>{{x.phone}}</td>\r\n                                        <td>{{x.address}}</td>\r\n                                        <td class=\"text-center\">\r\n                                            <button class=\"btn btn-outline-info mx-1\" v-on:click=\"edit=true\">\r\n                                                <i class=\"fa fa-edit\"></i>\r\n                                            </button>\r\n                                            <button class=\"btn btn-outline-danger mx-1\" type=\"button\" data-bs-toggle=\"modal\" data-bs-target=\"#exampleModal\">\r\n                                                <i class=\"fa fa-trash\"></i>\r\n                                            </button>\r\n                                            <router-link to=\"/profile\" class=\"btn btn-outline-dark mx-1\"><i class=\"fa fa-arrow-right\"></i></router-link>\r\n                                        </td>\r\n                                    </tr>\r\n                                </tbody>\r\n                            </table>\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <UsersAddComponent v-if=\"add\"/>\r\n                <UsersEditComponent v-if=\"edit\"/>\r\n                <!--END MAIN-->\r\n            </main>\r\n            <!--END ROW MAIN-->\r\n        </div>\r\n    </div>\r\n    <!-- Modal -->\r\n    <div class=\"modal fade\" id=\"exampleModal\" tabindex=\"-1\" aria-labelledby=\"exampleModalLabel\" aria-hidden=\"true\">\r\n        <div class=\"modal-dialog modal-dialog-centered\">\r\n            <div class=\"modal-content\">\r\n                <div class=\"modal-header\">\r\n                    <h1 class=\"modal-title fs-5\" id=\"exampleModalLabel\">Delete User</h1>\r\n                    <button type=\"button\" class=\"btn-close\" data-bs-dismiss=\"modal\" aria-label=\"Close\"></button>\r\n                </div>\r\n                <div class=\"modal-body\">\r\n                    Are you sure delete user?\r\n                </div>\r\n                <div class=\"modal-footer\">\r\n                    <button type=\"button\" class=\"btn btn-secondary\" data-bs-dismiss=\"modal\">Close</button>\r\n                    <button type=\"button\" class=\"btn btn-danger\">Delete</button>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import HeaderComponent from '@/components/layouts/HeaderComponent.vue';\r\n    import SidebarComponent from '@/components/layouts/SidebarComponent.vue';\r\n    import UsersAddComponent from './UsersAddComponent.vue';\r\n    import UsersEditComponent from './UsersEditComponent.vue';\r\n    import axios from 'axios';\r\n    export default{\r\n        name:'UsersComponent',\r\n        components: {\r\n            HeaderComponent,\r\n            SidebarComponent,\r\n            UsersAddComponent,\r\n            UsersEditComponent\r\n        },\r\n        data(){\r\n            return{\r\n                add:false,\r\n                edit:false,\r\n                data:[]           \r\n            }\r\n        },created(){\r\n            axios.get(\"http://localhost:8000/api/users\").then((result)=>{\r\n                if (result.data.status == 'success'){\r\n                    this.data = result.data.data\r\n                }\r\n            })\r\n        }\r\n    }\r\n</script>\r\n\r\n<style>\r\n    .img-Profile{\r\n        width: 50px;\r\n        height: 50px;\r\n        border-radius: 50%;\r\n        margin-right: 10px;\r\n    }\r\n</style>"],"mappings":"AAyFI,OAAOA,eAAc,MAAO,0CAA0C;AACtE,OAAOC,gBAAe,MAAO,2CAA2C;AACxE,OAAOC,iBAAgB,MAAO,yBAAyB;AACvD,OAAOC,kBAAiB,MAAO,0BAA0B;AACzD,OAAOC,KAAI,MAAO,OAAO;AACzB,eAAc;EACVC,IAAI,EAAC,gBAAgB;EACrBC,UAAU,EAAE;IACRN,eAAe;IACfC,gBAAgB;IAChBC,iBAAiB;IACjBC;EACJ,CAAC;EACDI,IAAIA,CAAA,EAAE;IACF,OAAM;MACFC,GAAG,EAAC,KAAK;MACTC,IAAI,EAAC,KAAK;MACVF,IAAI,EAAC;IACT;EACJ,CAAC;EAACG,OAAOA,CAAA,EAAE;IACPN,KAAK,CAACO,GAAG,CAAC,iCAAiC,CAAC,CAACC,IAAI,CAAEC,MAAM,IAAG;MACxD,IAAIA,MAAM,CAACN,IAAI,CAACO,MAAK,IAAK,SAAS,EAAC;QAChC,IAAI,CAACP,IAAG,GAAIM,MAAM,CAACN,IAAI,CAACA,IAAG;MAC/B;IACJ,CAAC;EACL;AACJ"},"metadata":{},"sourceType":"module","externalDependencies":[]}